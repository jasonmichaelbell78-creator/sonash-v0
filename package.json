    "docs:check": "node scripts/check-docs-light.js",
    "docs:archive": "node scripts/archive-doc.js",
    "docs:sync-check": "node scripts/check-document-sync.js",
    "docs:index": "node scripts/generate-documentation-index.js",
    "roadmap:validate": "node scripts/check-roadmap-health.js",
