    "aggregate:audit-findings": "node scripts/aggregate-audit-findings.js && prettier --write docs/aggregation/*.md",
    "security:check": "node scripts/security-check.js",
    "security:check-all": "node scripts/security-check.js --all",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lighthouse": "node scripts/lighthouse-audit.js",
    "lighthouse:desktop": "node scripts/lighthouse-audit.js --desktop",
    "deps:circular": "madge --circular --extensions ts,tsx,js,jsx lib/ components/ app/",
    "deps:unused": "knip",
    "prepare": "husky || echo 'Husky not available, skipping git hooks setup'",
    "hooks:test": "node scripts/test-hooks.js",
    "hooks:health": "node scripts/check-hook-health.js",
    "docs:headers": "node scripts/check-doc-headers.js --all"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": "prettier --write"
