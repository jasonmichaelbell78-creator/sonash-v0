    "learning:detailed": "node scripts/analyze-learning-effectiveness.js --detailed",
    "learning:category": "node scripts/analyze-learning-effectiveness.js --category",
    "learning:since": "node scripts/analyze-learning-effectiveness.js --since-review",
    "session:log": "node scripts/log-session-activity.js",
    "session:summary": "node scripts/log-session-activity.js --summary",
    "override:log": "node scripts/log-override.js",
    "override:list": "node scripts/log-override.js --list",
    "triggers:check": "node scripts/check-triggers.js",
    "crossdoc:check": "node scripts/check-cross-doc-deps.js",
    "skills:validate": "node scripts/validate-skill-config.js",
    "skills:verify-usage": "node scripts/verify-skill-usage.js",
    "validate:canon": "node scripts/validate-canon-schema.js",
    "audit:validate": "node scripts/validate-audit.js",
